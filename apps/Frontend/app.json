{
  "expo": {
    "name": "FitPal AI",
    "slug": "smartfit",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "platforms": ["ios", "android", "web"],
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#f8fafc"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#f8fafc"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "sdkVersion": "53.0.0"
  }
}
